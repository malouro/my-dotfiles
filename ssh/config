Host *
  IgnoreUnknown UseKeychain

Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  User git
  HostName github.com
  IdentityFile ~/.ssh/id_rsa_github
  StrictHostKeyChecking accept-new

Host bb.icimslabs.net
  AddKeysToAgent yes
  UseKeychain yes
  HostName bb.icimslabs.net
  IdentityFile ~/.ssh/id_rsa_icims
